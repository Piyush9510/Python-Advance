# Write a Python program to find HCF (GCD) of two numbers
n = int(input('Enter the first number: '))
m = int(input('Enter the second number: '))

for i in range(1,min(n,m)+1):
    if n % i == 0 and m % i == 0:
        c=i
print('HCF is',c)

# =======================================================

# Write a Python program to find LCM of two numbers.
n = int(input('Enter the first number: '))
m = int(input('Enter the second number: '))
k = 1
l = 1
k1 = []
l1 = []
for i in range(1,100):
    k=n*i
    k1.append(k)
for i in range(1,100):
    l = m * i
    l1.append(l)
k1 = set(k1)
k2 = set(l1)
c = k1.intersection(k2)
str(c)
print('LSM of',n,'and',m,'is ',sorted(c)[0])

# =============================================================

# Write a Python program to check whether a number is Prime number or  not.
n = int(input('Enter the number: '))
l = []
for i in range(1,n+1):
    m = n % i
    if m == 0:
      l.append(i)
if len(l) >= 3:
    print('Number is prime number')
else:
    print('Number is not prime number')
    
# ====================================================

# Write a Python program to print all Prime numbers between I to n.
n = int(input('Enter the number: '))
l = []
d = []
for i in range(1,n+1):
    for j in range(1,i+1):
        m = i % j
        if m == 0:
            l.append(j)
    if len(l) <= 2:
        l.clear()
        d.append(i)
    else:
        l.clear()
print(d)

# ===================================================================

# Write a Python program to find sum of all prime numbers between 1 to n
n = int(input('Enter the number: '))
l = []
d = []
for i in range(1,n+1):
    for j in range(1,i+1):
        m = i % j
        if m == 0:
            l.append(j)
    if len(l) <= 2:
        l.clear()
        d.append(i)
    else:
        l.clear()
print(d)
print(sum(d))

# =========================================================

# Write a Python program to check whether a number is Armstrong number or not.
n = input('Enter the number: ')
m = 0
for i in range(0,len(n)):
    m = m + int(n[i])**3
if m == int(n):
    print('Number is Armstrong')
else:
    print('Number is not Armstrong')
    
# =====================================================================

# Write a Python program to print Fibonacci series up to n terms.
n = int(input('Enter the number'))
sum = 0
s =  []
for i in range(0,2):
    a=i
    s.append(a)
for i in range(2,n+1):
    sum = s[-1]+s[-2]
    if sum < n:
        s.append(sum)
print(s)

